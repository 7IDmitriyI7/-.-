using System;

namespace Задание5._1
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Введите первую координату числа A");
            double a1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите вторую координату числа A");
            double a2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите первую координату числа B");
            double b1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите вторую координату числа B");
            double b2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите первую координату числа C");
            double c1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите вторую координату числа C");
            double c2 = Convert.ToDouble(Console.ReadLine());
            double AC=Math.Sqrt(Math.Pow((c1 - a1), 2) + Math.Pow((c2 - a2), 2));
            double BC = Math.Sqrt(Math.Pow((c1 - b1), 2) + Math.Pow((c2 - b2), 2));
            double SumAC = AC+BC;
            Console.WriteLine("Длина отрезака АС= " + AC);
            Console.WriteLine("Длина отрезака BС= " + BC);
            Console.WriteLine("Сумма длин отрезков АС+ BC= " + SumAC);
        }
    }
}
